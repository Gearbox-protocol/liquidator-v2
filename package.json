  "dependencies": {
    "node-pty": "^1.0.0",
    "pino-pretty": "^13.1.1"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.859.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@flashbots/ethers-provider-bundle": "^1.0.0",
    "@gearbox-protocol/eslint-config": "2.0.0-next.2",
    "@gearbox-protocol/liquidator-v2-contracts": "^2.4.0",
    "@gearbox-protocol/prettier-config": "2.1.0",
    "@gearbox-protocol/sdk-gov": "2.34.0-next.112",
    "@gearbox-protocol/types": "^1.14.8",
    "@redstone-finance/evm-connector": "^0.7.3",
    "@types/node": "^24.2.0",
    "@uniswap/sdk-core": "^7.7.2",
    "@uniswap/v3-sdk": "^3.25.2",
    "@vlad-yakovlev/telegram-md": "^2.1.0",
    "abitype": "^1.0.8",
    "axios": "^1.11.0",
    "axios-retry": "^4.5.0",
    "date-fns": "^4.1.0",
    "di-at-home": "^0.0.7",
    "dotenv": "^17.2.1",
    "esbuild": "^0.25.8",
    "eslint": "^8.57.0",
    "ethers": "^6.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.4",
    "nanoid": "^5.1.5",
    "pino": "^9.7.0",
    "prettier": "^3.6.2",
    "redstone-protocol": "^1.0.5",
    "tsx": "^4.20.3",
    "typescript": "^5.9.2",
    "viem": "^2.33.2",
    "vitest": "^3.2.4"
