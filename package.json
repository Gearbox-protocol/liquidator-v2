{
  "name": "@gearbox-protocol/liquidator-v2",
  "description": "Gearbox liquidation bot",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "clean": "rm -rf build",
    "build": "tsc --p tsconfig.build.json",
    "start": "ts-node -r dotenv/config src/index.ts",
    "prepare": "husky",
    "prettier": "prettier --write .",
    "prettier:ci": "npx prettier --check .",
    "lint": "eslint \"**/*.ts\" --fix",
    "lint:ci": "eslint \"**/*.ts\"",
    "typecheck:ci": "tsc --noEmit",
    "test": "vitest",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.583.0",
    "@flashbots/ethers-provider-bundle": "^1.0.0",
    "@gearbox-protocol/liquidator-v2-contracts": "^2.1.0-next.12",
    "@gearbox-protocol/sdk-gov": "^2.3.1",
    "@gearbox-protocol/types": "^1.7.2",
    "@redstone-finance/evm-connector": "^0.5.1",
    "@uniswap/sdk-core": "^5.0.0",
    "@uniswap/v3-sdk": "^3.11.1",
    "axios": "^1.7.2",
    "axios-retry": "^4.3.0",
    "date-fns": "^3.6.0",
    "ethers": "^6.12.1",
    "ethers-decode-error": "^2.1.3",
    "patch-package": "^8.0.0",
    "pino": "^9.1.0",
    "postinstall-postinstall": "^2.1.0",
    "redstone-protocol": "^1.0.5",
    "reflect-metadata": "^0.2.2",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@gearbox-protocol/eslint-config": "2.0.0-next.2",
    "@gearbox-protocol/prettier-config": "2.0.0-next.0",
    "@types/node": "^20.12.12",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "pino-pretty": "^11.1.0",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^1.5.2"
  },
  "prettier": "@gearbox-protocol/prettier-config",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  }
}
