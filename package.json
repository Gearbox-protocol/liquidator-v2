{
  "name": "@gearbox-protocol/liquidator-v2",
  "description": "Gearbox liquidation bot",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf build",
    "build": "node esbuild.config.mjs",
    "start": "tsx --env-file .env src/index.ts | pino-pretty --colorize",
    "prepare": "husky",
    "prettier": "prettier --write .",
    "prettier:ci": "npx prettier --check .",
    "lint": "eslint \"**/*.ts\" --fix",
    "lint:ci": "eslint \"**/*.ts\"",
    "typecheck:ci": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@gearbox-protocol/cli-utils": "^5.36.7",
    "node-pty": "^1.0.0",
    "pino-pretty": "^13.0.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.828.0",
    "@commander-js/extra-typings": "^14.0.0",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@gearbox-protocol/eslint-config": "2.0.0-next.2",
    "@gearbox-protocol/liquidator-v2-contracts": "^2.4.0",
    "@gearbox-protocol/next-contracts": "npm:@gearbox-protocol/liquidator-v2-contracts@2.5.0-next.3",
    "@gearbox-protocol/prettier-config": "2.1.0",
    "@gearbox-protocol/sdk": "8.0.4",
    "@gearbox-protocol/types": "^1.14.8",
    "@types/node": "^24.0.1",
    "@uniswap/sdk-core": "^7.7.2",
    "@uniswap/v3-sdk": "^3.25.2",
    "@vlad-yakovlev/telegram-md": "^2.0.0",
    "abitype": "^1.0.8",
    "axios": "^1.9.0",
    "axios-retry": "^4.5.0",
    "date-fns": "^4.1.0",
    "di-at-home": "^0.0.7",
    "esbuild": "^0.25.5",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "nanoid": "^5.1.5",
    "node-pty": "^1.0.0",
    "pino": "^9.7.0",
    "prettier": "^3.5.3",
    "tsx": "4.20.1",
    "typescript": "^5.8.3",
    "viem": "^2.31.1",
    "vitest": "^3.2.3",
    "zod": "^3.25.63"
  },
  "prettier": "@gearbox-protocol/prettier-config",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "packageManager": "yarn@4.9.2"
}
