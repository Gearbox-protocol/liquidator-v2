    "@types/express": "^4.17.20",
    "@uniswap/sdk-core": "^4.1.2",
    "@uniswap/v3-sdk": "^3.10.2",
    "amqplib": "^0.10.3",
    "async-mutex": "^0.4.1",
    "axios": "^1.6.7",
