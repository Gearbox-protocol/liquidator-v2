    "eslint": "^8.57.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "nanoid": "^5.0.9",
    "node-pty": "^1.0.0",
    "pino": "^9.6.0",
    "prettier": "^3.4.2",
