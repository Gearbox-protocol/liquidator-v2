    "@uniswap/sdk-core": "^4.0.7",
    "@uniswap/v3-sdk": "^3.10.0",
    "amqplib": "^0.10.3",
    "async-mutex": "^0.4.0",
    "axios": "^1.5.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
