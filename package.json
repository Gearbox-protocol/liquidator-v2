    "axios-retry": "^4.5.0",
    "date-fns": "^4.1.0",
    "di-at-home": "^0.0.7",
    "dotenv": "^16.4.5",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.0",
    "ethers": "^6.13.4",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nanoid": "^5.0.8",
    "node-pty": "^1.0.0",
    "pino": "^9.5.0",
    "prettier": "^3.3.3",
    "redstone-protocol": "^1.0.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
