  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.2",
    "@gearbox-protocol/eslint-config": "2.0.0-next.2",
    "@gearbox-protocol/prettier-config": "2.0.0-next.0",
    "@types/amqplib": "^0.10.3",
    "@types/node": "^20.11.17",
    "eslint": "^8.52.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^1.2.2"
