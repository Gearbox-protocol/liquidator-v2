{
  "name": "@gearbox-protocol/liquidator-v2",
  "description": "Gearbox liquidation bot",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf build",
    "build": "node esbuild.config.mjs",
    "start": "tsx --env-file .env src/index.ts | pino-pretty --colorize",
    "prepare": "husky",
    "prettier": "prettier --write .",
    "prettier:ci": "npx prettier --check .",
    "lint": "eslint \"**/*.ts\" --fix",
    "lint:ci": "eslint \"**/*.ts\"",
    "typecheck:ci": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "node-pty": "^1.0.0",
    "pino-pretty": "^13.0.0",
    "zod-validation-error": "^3.4.0"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.758.0",
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@gearbox-protocol/eslint-config": "2.0.0-next.2",
    "@gearbox-protocol/liquidator-v2-contracts": "^2.3.2",
    "@gearbox-protocol/prettier-config": "2.0.0",
    "@gearbox-protocol/sdk": "3.0.0-vfour.280",
    "@gearbox-protocol/types": "^1.14.6",
    "@types/node": "^22.13.9",
    "@uniswap/sdk-core": "^7.7.0",
    "@uniswap/v3-sdk": "^3.25.0",
    "@vlad-yakovlev/telegram-md": "^2.0.0",
    "abitype": "^1.0.8",
    "axios": "^1.8.1",
    "axios-retry": "^4.5.0",
    "date-fns": "^4.1.0",
    "di-at-home": "^0.0.7",
    "dotenv": "^16.4.7",
    "esbuild": "^0.25.0",
    "eslint": "^8.57.1",
    "husky": "^9.1.7",
    "lint-staged": "^15.4.3",
    "nanoid": "^5.1.2",
    "node-pty": "^1.0.0",
    "pino": "^9.6.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "viem": "^2.23.6",
    "vitest": "^3.0.7"
  },
  "prettier": "@gearbox-protocol/prettier-config",
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": "prettier --write"
  },
  "packageManager": "yarn@4.6.0"
}
