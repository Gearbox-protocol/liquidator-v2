    "@commitlint/config-conventional": "^17.7.0",
    "@gearbox-protocol/eslint-config": "2.0.0-next.0",
    "@gearbox-protocol/prettier-config": "2.0.0-next.0",
    "@types/amqplib": "^0.10.2",
    "@types/chai": "^4.3.7",
    "@types/mocha": "^10.0.2",
    "@types/node": "^20.8.4",
    "chai": "^4.3.10",
    "eslint": "^8.51.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
